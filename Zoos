#include <iostream>
using namespace std;

int main(){
    int x=0,y=0;
    char s[20];
    cin>>s;
    for(int i=0; i<20; i++){
        if(s[i]=='z'){
            x++;
        }
        else if(s[i]=='o'){
            y++;
        }
        else
        break;
    }
    int n=2*x;
    if(n==y)
        cout<<"Yes";
    else
        cout<<"No";
}
